@model MediaReferenceList<MediaReference>
@using EPiServer.Editor
@using HtmlAgilityPack
@using ImageVault.EPiServer
@using ImageVault.EPiServer.UI

@{
    var client = ImageVaultClient.GetPageClient();
    List<DisplayMediaItem> items;
    Exception exception = null;
    try
    {
        items = client.Load<DisplayMediaItem>(Model, ViewContext).ToList();

    } catch (Exception e)
    {
        DisplayMediaItem.Log.Error("Failed to load media list",e);
        exception = e;
        items = null;
    }
}

@functions {
    /// <summary>
    /// Helper function to render the HTML of the supplied WebMedia
    /// If included in ViewData, attributes class, alt and title will be added/updated.
    /// </summary>
    /// <param name="item">The WebMedia item to render</param>
    /// <param name="viewDataItem">The dictionary</param>
    /// <returns></returns>
    private string RenderTag(DisplayMediaItem item, IDictionary<string, string> viewDataItem)
    {
        var mediaHtml = new HtmlDocument();
        var addLink = false;
        if (item.WebMedia != null && item.WebMedia.Html != null)
        {
            mediaHtml.LoadHtml(item.WebMedia.Html);
        }
        else if (item.Thumbnail != null && item.Thumbnail.Html != null)
        {
            mediaHtml.LoadHtml(item.Thumbnail.Html);
            addLink = true;
        }
        else
        {
            mediaHtml.LoadHtml("<span>" + item.Name + "</span>");
            addLink = true;
        }

        var tag = mediaHtml.DocumentNode.FirstChild;
        var attribs = tag.Attributes;

        if (viewDataItem != null)
        {
            if (ViewData["class"] != null)
            {
                AddAttribute(attribs, "class", viewDataItem["class"]);
            }

            if (ViewData["alt"] != null)
            {
                AddAttribute(attribs, "alt", viewDataItem["alt"]);
            }

            if (ViewData["title"] != null)
            {
                AddAttribute(attribs, "title", viewDataItem["title"]);
            }
        }
        if (addLink)
        {
            var contentHtml = tag.OuterHtml;
            mediaHtml.LoadHtml("<a>" + contentHtml + "</a>");
            tag = mediaHtml.DocumentNode.FirstChild;
            AddAttribute(tag.Attributes, "href", item.Original.Url);
        }
        return tag.OuterHtml;
    }
    /// <summary>
    /// Adds or updates an attribute based on the supplied parameters
    /// </summary>
    /// <param name="attribs">The collection of attributes to add to or update</param>
    /// <param name="attributeName">The attribute name</param>
    /// <param name="attributeValue">The attribute value</param>
    private void AddAttribute(HtmlAttributeCollection attribs, string attributeName, string attributeValue)
    {
        if (attribs[attributeName] != null)
        {
            attribs[attributeName].Value = attributeValue;
        }
        else
        {
            attribs.Add(attributeName, attributeValue);
        }
    }
}

@if (items!=null && items.Count != 0)
{
    IList<IDictionary<string, string>> attributeList = null;

    var enu = (IEnumerable<IDictionary<string, string>>)ViewData["attributeList"];
    if (enu != null)
    {
        attributeList = enu.ToList();
    }


    for (var i = 0; i < items.Count; i++)
    {
        var item = items[i];

        if (item != null)
        {
            IDictionary<string, string> viewDataItem = null;
            if (attributeList != null && i >= 0 && i < attributeList.Count)
            {
                viewDataItem = attributeList.ElementAt(i);
            }

            @Html.Raw(RenderTag(items[i], viewDataItem))
        }
        else if (PageEditing.PageIsInEditMode)
        {
            // If media is deleted or user has no access
            <div class="ivEmptyActions" style="padding-top: 150px; color: #303030; text-shadow: none; background-color: #fff; text-align: center; width: 100%; display: inline-block; background-position: 50% 30px; background-repeat: no-repeat; background-image: url('data:image/png;base64,');">
                <p style="font-weight: bold;">Cannot find media in ImageVault. It's either removed or you don't have access.</p>
            </div>
        }
    }
}
else if (PageEditing.PageIsInEditMode)
{
    if (exception != null)
     {
         <div class="ivEmptyActions" style="padding-top: 150px; color: #303030; text-shadow: none; background-color: #fff; text-align: center; width: 100%; display: inline-block; background-position: 50% 30px; background-repeat: no-repeat; background-image: url('data:image/png;base64,');">
             <p style="font-weight: bold;">Error occurred: @exception.Message<!--
@exception.StackTrace
--></p>
         </div>

     } else
     {
         <div class="ivEmptyActions" style="padding-top: 150px; color: #303030; text-shadow: none; background-color: #fff; text-align: center; width: 100%; display: inline-block; background-position: 50% 30px; background-repeat: no-repeat; background-image: url('data:image/png;base64,');">
             <p style="font-weight: bold;">Add ImageVault media on the property page.</p>
         </div>
     }
}